<header>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
  <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/themes/smoothness/jquery-ui.css" />
  <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
  <script>
                          // 2. This code loads the IFrame Player API code asynchronously.
                          var tag = document.createElement('script');
                    
                          tag.src = "https://www.youtube.com/iframe_api";
                          var firstScriptTag = document.getElementsByTagName('script')[0];
                          firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
                    
                          // 3. This function creates an <iframe> (and YouTube player)
                          //    after the API code downloads.
                          var player;
                          function onYouTubeIframeAPIReady() {
                            player = new YT.Player('trailer_player', {
                              videoId: 'udDIkl6z8X0',
							  playerVars: {'autohide': 1, 'modestbranding':1 },
                              events: {
                                'onReady': onPlayerReady,
                                'onStateChange': onPlayerStateChange
                              }
                            });
                          }
                    
                          // 4. The API will call this function when the video player is ready.
                          function onPlayerReady(event) {
                          }

						  // 5. The API calls this function when the player's state changes.
						  //    The function indicates that when playing a video (state=1),
						  //    the player should play for six seconds and then stop.
						  function onPlayerStateChange(event) {
							if (event.data == YT.PlayerState.FINISHED) {
							  $('#infobox_trailer').fadeOut();
							}
						  }
						 
   </script>
  <script>
	$('body').on({
		mouseenter: function(){
			$(this).parent().stop().css('background-color','rgb(243, 243, 243)');
			$(this).parent().find('.rank_number').animate({'opacity':'1'},400);
			},
		mouseleave: function(){
			$(this).parent().stop().css('background-color','white');
			$(this).parent().find('.rank_number').stop().css('opacity','0');
			},
		click: function(){
				//Account for scrollbar movement
				if (document.body.scrollHeight > document.body.clientHeight){
					$('body').css({'padding-right':'15px'});
					$('#menu_box').css({'right': 45});
				}
				$('body').css({'overflow-y':'hidden'});
				$('#overlay').fadeIn(400);
			}
		},'.content_centering')

<!-- Closing Overlay-->
	$('body').on({
			click: function(e){
				if($("#overlay_container").has(e.target).length === 0){			
					$('#overlay').fadeOut(400,'',function(){
						//Account for scrollbar movement
						
						if (document.body.scrollHeight > document.body.clientHeight){
							$('body').css({'padding-right':''});
							$('#menu_box').css({'right': 30})
						}
						
						
						$('body').css({'overflow-y':''});
						
					});
					$('#infobox_trailer').fadeOut();
					player.stopVideo();	
				}
			}
		}, '#overlay')
		

<!-- Open trailer + hovering -->
	$('body').on({
		mouseenter: function(){
				$('#poster_overlay').css('opacity','0.5');
			},
		mouseleave: function(){
				$('#poster_overlay').css('opacity','0');
			},
		click: function(e){		
				$('#poster_overlay').css('opacity','0');			
				setTimeout(function(){$('#infobox_trailer').fadeIn(400,'',initiate_trailer());}, 50);
			}
		}, '#infobox_poster')


<!-- Close trailer -->
	$('body').on({
			click: function(){		
					$('#infobox_trailer').fadeOut(400,'',player.stopVideo());
			}
		}, '#infobox_trailer')
	

<!-- Close trailer hovering -->

	$('body').on({
			mouseenter: function(){		
				$('#trailer_close_button').css('color','#c72109')
			},
			mouseleave: function(){		
				$('#trailer_close_button').css('color','')
			}
		}, '#trailer_close_background')



<!-- Consuming hovering -->

	$('body').on({
			mouseenter: function(){		
				$(this).css({'background-color':'#c72109','color':'white'})
			},
			mouseleave: function(){		
				$(this).css({'background-color':'','color':''})
			}
		}, '.overlay_half_block')


<!-- Overlay Avatar hovering -->

	$('body').on({
			mouseenter: function(){		
				$(this).css({'opacity':'.5'})
			},
			mouseleave: function(){		
				$(this).css({'opacity':'1'})
			}
		}, '.overlay_user_avatar')



function initiate_trailer(){
	player.playVideo();
}


function collapse_cell(){
	$('.rank_container').css('left',0);
	$('.rank_number').css('background-color','rgba(0,0,0,0.2)');
	$('.data').hide();
	$('.cell').css({'width':'100px','display':'inline-block'})
	$('#content').css({'max-width':925,'margin':'0 auto'})
	$('.numeral_divider').hide();
	$('.rating_divider').show();
}

function expand_cell(){
	$('.rank_container').css('left',-100);
	$('.rank_number').css('background-color','');
	$('.data').css('display','inline-block');
	$('.cell').css({'width':'','display':''})
	$('#content').css({'max-width':'','margin':''})
	$('.numeral_divider').show();
	$('.rating_divider').hide();
}


	
</script>
